{
    "name": "cerbero/octane-testbench",
    "type": "library",
    "description": "Set of utilities to test Laravel applications powered by Octane.",
    "keywords": [
        "laravel",
        "octane",
        "laravel-octane",
        "test",
        "testing"
    ],
    "homepage": "https://github.com/cerbero90/octane-testbench",
    "license": "MIT",
    "authors": [{
        "name": "Andrea Marco Sartori",
        "email": "andrea.marco.sartori@gmail.com",
        "homepage": "https://github.com/cerbero90",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.0",
        "laravel/octane": "^1.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": ">=9.3"
    },
    "require-dev": {
        "orchestra/testbench": ">=6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Cerbero\\OctaneTestbench\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerbero\\OctaneTestbench\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs --standard=PSR12 src tests",
        "fix-style": "phpcbf --standard=PSR12 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
